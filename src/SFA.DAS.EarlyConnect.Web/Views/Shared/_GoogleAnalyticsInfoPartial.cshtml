    var surveyId = ViewBag.GaData?.SurveyId != null ? ViewBag.GaData.SurveyId : null;
    var acc = ViewBag.GaData?.Acc != null ? ViewBag.GaData.Acc : null;
    var isAuthenticated = User.Identity.IsAuthenticated ? "True" : "False";
}
<script>
    window.dataLayer = window.dataLayer || [];
    dataLayer.push({
        'event': 'dataLoaded',
        'loggedIn': '@isAuthenticated',
        'vpv': '@vpv',
        'surveyId': '@surveyId'
    });
</script>